{
    "name": "MLX Exporter",
    "uniqueId": "mlx_exporter",
    "description": "Exports the current model to MLX format so it can be run on Apple Silicon.",
    "plugin-format": "python",
    "type": "exporter",
    "model_architectures": [
        "LlamaForCausalLM",
        "MistralForCausalLM",
        "PhiForCausalLM"
    ],
    "export_architecture": "MLX",
    "files": [
        "main.py",
        "setup.sh"
    ],
    "setup-script": "setup.sh",
    "parameters": [{
        "quant_bits": {
            "title": "Quantization Bits",
            "type": "integer",
            "default": 4,
            "minimum": 1
        }
    }]
}
